@model QuanHuyenModel
@using Nop.Admin.Models.ChonVes 
@{
    //page title
    ViewBag.Title = T("Admin.Configuration.QuanHuyen.EditDetails").Text;
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Administration/Content/images/ico-configuration.png")" alt="" />
            @T("Admin.Configuration.QuanHuyen.EditDetails") - @Model.Ten @Html.ActionLink("(" + @T("AdminAdmin.Configuration.QuanHuyen.BackToList") + ")", "List")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
            <span id="quanhuyen-delete" class="k-button">@T("Admin.Common.Delete")</span>
        </div>
    </div>
    @Html.Partial("_CreateOrUpdate", Model)
}
@Html.DeleteConfirmation("quanhuyen-delete")
