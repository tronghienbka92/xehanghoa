@model PhieuGuiHangModel
@using Nop.Web.Models.NhaXes
@{
    Layout = "~/Views/Shared/_NhaXeLayout.cshtml";
    //page title
    ViewBag.Title ="Tạo phiếu gửi hàng";
}
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "PhieuGuiHang-form" }))
{
    <div class="section-header">
        <div class="title">
            <img src="@Url.Content("~/Content/chonve/nxmenu/hanhtrinhlichtrinh.png")" alt="" />
            @T("ChonVe.NhaXe.PhieuGuiHang.TaoMoi") @Html.ActionLink("(Quay về danh sách phiếu gửi hàng)", "QLGuiHang")
        </div>
        <div class="options">
            <input type="submit" name="save" class="k-button" value="@T("Admin.Common.Save")" />
            <input type="submit" name="save-continue" class="k-button" value="@T("Admin.Common.SaveContinue")" />
        </div>
    </div>
    @Html.Partial("_PhieuGuiHangChinhSua", Model)
}